<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
	<class name="Student" table="student">
		<id name="studentid" column="StudentId" type="integer">
			<generator class="native"/>
		</id>
		<property name="studentnumber" column="StudentNumber" type="string"></property>
		<property name="studentname" column="StudentName" type="string"></property>
		<property name="studenttag" column="StudentTag" type="string"></property>
		<property name="status" column="StudentStatus" type="string"></property>
		<property name="ipAdress" column="IpAdress" type="string"></property>
		<property name="uploadFileName" column="UploadFileName" type="string"></property>
		<property name="uploadFileDate" column="UploadFileDate" type="string"></property>
	</class>
</hibernate-mapping>